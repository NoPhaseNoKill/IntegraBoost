<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss} - %C - %m%n</pattern>
        </encoder>
    </appender>

    <!--    sets this package to info-->
    <logger name="com.integraboost" level="DEBUG"/>

    <!--    sets everything else to warn-->
    <root level="ERROR">
        <appender-ref ref="STDOUT"/>
    </root>

</configuration>

